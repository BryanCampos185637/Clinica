<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Clinica</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link href="~/css/navbar.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link href="~/lib/limonte-sweetalert2/sweetalert2.min.css" rel="stylesheet" />
</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-dark fixed-top bg-primary">
        <a class="navbar-brand text-dark" href="#">Version: Beta</a>
        <button class="navbar-toggler"
                type="button"
                data-toggle="collapse"
                data-target="#navbarCollapse"
                aria-controls="navbarCollapse"
                aria-expanded="false"
                aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarCollapse">
            <ul class="navbar-nav mr-auto sidenav" id="navAccordion">
                <li class="nav-item">
                    <a class="nav-link text-dark" asp-area="" asp-controller="Servicio" asp-action="Index">Servicio</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link text-dark" asp-area="" asp-controller="Enfermedad" asp-action="Index">Enfermedad</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link text-dark" asp-area="" asp-controller="Paciente" asp-action="Index">Paciente</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link text-dark" asp-area="" asp-controller="Expediente" asp-action="Index">Expedientes</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link text-dark" asp-area="" asp-controller="Cita" asp-action="CrearCita">Crear cita</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link text-dark" asp-area="" asp-controller="Cita" asp-action="Index">Citas creadas</a>
                </li>
                <li class="nav-item">
                    <a href="#" class="nav-link text-dark" onclick="CerrarSesion()">Cerrar sesion</a>
                </li>
            </ul>
            <div class="form-inline ml-auto mt-2 mt-md-0">
                <h4 class="text-dark">@ViewData["Title"] </h4>
            </div>
        </div>
    </nav>

    <main class="content-wrapper">
        <div class="container-fluid">
            <div class="container">
                @RenderBody()
                <div class="row">
                    <div class="col-12">
                        <div class="text-center">
                            <span>Hecho por: DevSpartan</span><br />
                            <span>Contactanos: dev.spartan.sv@gmail.com</span><br />
                            <a href="https://www.myASP.NET/index?r=Devspartansv">
                                <img src="https://www.myASP.NET/affiliate/728X90.gif" border="0">
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </main>





