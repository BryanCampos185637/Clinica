    <div style="display:none">
        <a class="nav-link text-dark" asp-area="" asp-controller="Account" asp-action="Cerrar" id="btnCerrarSesion"></a>
    </div>
    @if (TempData["success"] != null || TempData["warning"] != null)
    {
        if (TempData["success"] != null)
        {
            <input type="hidden" id="txtMensajeServidor" value="@TempData["success"]" />
        }
        if (TempData["warning"] != null)
        {
            <input type="hidden" id="txtMensajeServidor" value="@TempData["warning"]" />
        }
        <input type="hidden" id="txtIconoServer" value="@TempData["icono"]" />
    }
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/limonte-sweetalert2/sweetalert2.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
