@model DataAccessLogic.LogicaUsuario.AgregarUsuario.Ejecuta

@{
    ViewData["Title"] = "Guardar";
}

<h1>Guardar</h1>

<h4>Usuario</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Guardar">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="NombreUsuario" class="control-label"></label>
                <input asp-for="NombreUsuario" class="form-control" />
                <span asp-validation-for="NombreUsuario" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Contra" class="control-label"></label>
                <input asp-for="Contra" class="form-control" />
                <span asp-validation-for="Contra" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TipoUsuarioId" class="control-label"></label>
                <select asp-for="TipoUsuarioId" class="form-control" id="TipoUsuarioId">
                </select>
                <span asp-validation-for="TipoUsuarioId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

<script src="~/js/Peticiones AJAX/UsuarioAJAX.js"></script>