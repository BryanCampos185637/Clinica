@{
    ViewData["Title"] = "Inicio";
}

<div class="text-center">
    @if (ViewBag.Nombre != null)
    {
        <h1>SISTEMA CLINICA&reg;</h1>
        <h6>BIENVENIDO @ViewBag.Nombre </h6>
    }
    else
    {
        <h1>CLINICA @DateTime.Now.ToShortDateString()</h1>
        <h3>Bienvenido</h3>
    }


    <p>Sistema de clinica <strong>Versión: Beta</strong></p>
</div>
